<?xml version="1.0" encoding="UTF-8"?>
<access-policy>
	<cross-domain-access>
	<policy>
		<allow-from http-request-headers="*">
			<domain uri="*"></domain>
	</allow-from>
	<grant-to>
		<resource include-subpaths="true" path="/">
		</resource>
	</grant-to>
	</policy>
	</cross-domain-access>
</access-policy>