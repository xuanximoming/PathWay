<UserControl x:Class="YidanEHRApplication.Views.UserControls.UCVariationCheckBox"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="30" d:DesignWidth="100">

    <Grid x:Name="LayoutRoot" Background="Transparent">
        <CheckBox IsChecked="{Binding IsSelected,Mode=TwoWay}" Content="{Binding Bymc}" Margin="5" Checked="CheckBox_Checked" Unchecked="CheckBox_Unchecked"></CheckBox>
    </Grid>
</UserControl>
