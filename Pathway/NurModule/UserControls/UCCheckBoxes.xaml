<UserControl x:Class="YidanEHRApplication.NurModule.UserControls.UCCheckBoxes"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="30" d:DesignWidth="100">
    
    <Grid x:Name="LayoutRoot" Background="Transparent">
        <CheckBox Name="ckbResult" IsChecked="{Binding IsSelected,Mode=TwoWay}" Tag="{Binding}" Content="{Binding JgName}" Margin="5,2,5,5" Checked="CheckBoxes_Checked" Unchecked="CheckBoxes_Unchecked"></CheckBox>
    </Grid>
</UserControl>
