        <JobType>Repeat</JobType>
        <Frequency>
          <Interval>1</Interval>
          <IntervalUnit>Day</IntervalUnit>
        </Frequency>
        <FrequencyPerDay>
          <Repeatly>false</Repeatly>
          <TimeOfExecOnce>02:30:00</TimeOfExecOnce>
        </FrequencyPerDay>
        <Duration>
          <StartDate>2010-08-01</StartDate>
          <HasEndDate>false</HasEndDate>
        </Duration>
      </JobSchedule>
    </Job>
    <Job Name="住院病人和床位数据同步" Description="从HIS读取病人和床位数据" Enable="true" Visible="true" Class="YidanSoft.JobManager.PatInfoSynchronous" Library="YidanSoft.JobTasks.dll">
      <JobSchedule>
        <JobType>Repeat</JobType>
        <Frequency>
          <Interval>1</Interval>
          <IntervalUnit>Day</IntervalUnit>
        </Frequency>
        <FrequencyPerDay>
          <Repeatly>true</Repeatly>
          <Interval>5</Interval>
          <IntervalUnit>Minute</IntervalUnit>
          <StartTime>00:00:00</StartTime>
          <EndTime>23:59:59</EndTime>
        </FrequencyPerDay>
        <Duration>
          <StartDate>2010-08-01</StartDate>
          <HasEndDate>false</HasEndDate>
        </Duration>
      </JobSchedule>
    </Job>
